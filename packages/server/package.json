{
  "name": "@childcare-hub/server",
  "version": "0.1.0",
  "private": true,
  "author": "PauloIVM",
  "scripts": {
    "build": "yarn create:orm:file && tsc && tsc-alias -p tsconfig.build.json",
    "start": "node ./build/index.js",
    "dev": "nodemon --watch 'src/' --exec 'yarn build && yarn start' -e .ts",
    "test": "jest --forceExit --coverage --no-cache --verbose",
    "lint": "eslint src -c .eslintrc.json --no-eslintrc",
    ":typeorm": "npx typeorm --config src/ormconfig.json",
    "migration:run": "yarn build && yarn :typeorm migration:run",
    "migration:create": "yarn :typeorm migration:create",
    "start:debug": "nodemon --inspect src/index.ts",
    "create:orm:file": "test -f src/ormconfig.json || cp src/ormconfig.exemple.json src/ormconfig.json"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.5",
    "@types/fs-extra": "9.0.7",
    "@types/ioredis": "^4.17.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "tsc-alias": "^1.8.8",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "fs-extra": "^11.1.0",
    "ioredis": "^4.19.2",
    "joi": "^17.6.3",
    "mysql2": "^2.2.5",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  }
}
